{
  "compilerOptions": {
    "module": "ES6",
    "target": "ES5",
    "sourceMap": true,
    "jsx": "react",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "allowJs": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": [
      "DOM",
      "ES2019"
    ]
  }
}
